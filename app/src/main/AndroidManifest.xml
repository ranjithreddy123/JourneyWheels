<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.journeywheels">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="JourneyWheels"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".Bike_Home" />
        <activity android:name=".Aprilia" />
        <activity android:name=".Aprilia1" />
        <activity android:name=".Aprilia2" />
        <activity android:name=".Honda_Activa" />
        <activity android:name=".Honda_Activa1" />
        <activity android:name=".Honda_Activa2" />
        <activity android:name=".Yamaha" />
        <activity android:name=".Yamaha1" />
        <activity android:name=".Yamaha2" />
        <activity android:name=".FZ" />
        <activity android:name=".FZ1" />
        <activity android:name=".FZ2" />

        <activity android:name=".Register" android:label="Sign up" android:parentActivityName=".Login">
            - <!--  Parent activity meta-data to support 4.0 and lower
  -->
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".Login" />
        </activity>
        <activity android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar"
            />


    </application>

</manifest>